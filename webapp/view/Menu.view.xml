<mvc:View xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="sap.support.sccd.controller.Menu"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page>
		<Tree
			items="{
				path: 'menu>/'
			}">
			<StandardTreeItem
                title="{text}"
				icon="{ref}"/>
		</Tree>
	</Page>
</mvc:View>