{
	"_version": "1.4.0",
	"sap.app": {
		"id": "${project.artifactId}",
		"type": "application",
		"resources": "resources.json",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${project.version}"
		},
		"ach": "ach",
		"dataSources": {
			"mainService": {
				"uri": "/service/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.fiori": {
		"registrationIds": [ "F0000" ],
		"archeType": "transactional"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://business-objects-experience",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.support.sccd.view.App",
			"type": "XML",
			"id": "splitapp_sccd"
		},
		"dependencies": {
			"minUI5Version": "${sap.ui5.dist.version}",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				},
				"sap.ushell": {
					"minVersion": "1.38.0"
				},
				"sap.collaboration": {
					"minVersion": "1.38",
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.support.sccd.i18n.i18n"
				}
			},
			"config": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/config.json"
			},
			"menu": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/menu.json"
			},
			"kpi": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockdata/kpi.json"
			},
			"utoverview": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockdata/ut.json"
			},
			"itoverview": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockdata/it.json"
			},
			"utproject": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockdata/project_ut.json"
			},
			"itproject": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockdata/project_it.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.support.sccd.view",
				"controlId": "splitapp_sccd",
				"controlAggregation": "detailPages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": ["master"]
				},
				{
					"pattern": "home",
					"name": "home",
					"target": ["master", "home"]
				},
				{
					"pattern": "utov",
					"name": "utov",
					"target": ["master", "utoverview"]
				},
				{
					"pattern": "itov",
					"name": "itov",
					"target": ["master", "itoverview"]
				},
				{
					"pattern": "project/{pid}/{testtype}",
					"name": "project",
					"target": ["master", "project"]
				}
			],
			"targets": {
				"master": {
					"viewName": "Menu",
					"viewLevel": 0,
					"controlAggregation": "masterPages"
				},
				"home": {
					"viewName": "Home",
					"viewLevel": 0,
					"controlAggregation": "detailPages"
				},
				"utoverview": {
					"viewName": "ut.UTOv",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"itoverview": {
					"viewName": "it.ITOv",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"project": {
					"viewName": "Project",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				}
			}
		}
	}
}